# VS Code Marketplace Publishing Secrets
# Copy this file to .env and fill in your values
# DO NOT COMMIT .env TO GIT

# Personal Access Token (PAT) for VS Code Marketplace
# Generate at: https://dev.azure.com -> User Settings -> Personal Access Tokens
# Required scope: Marketplace (Publish)
VSCE_PAT=your_personal_access_token_here

# Publisher ID (from marketplace.visualstudio.com/manage)
VSCE_PUBLISHER=your_publisher_id_here
